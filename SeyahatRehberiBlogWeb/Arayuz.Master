<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Arayuz.master.cs" Inherits="SeyahatRehberiBlogWeb.Arayuz" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title>SeyahatRehberi</title>
    <link href="assests/css/Arayuzstyle.css" rel="stylesheet" />
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <form id="form1" runat="server">
        <div>
            <div class="UstPanel ">
                <a href="Default.aspx">ANASAYFA</a>
                <a href="Hakkimizda.aspx">HAKKIMIZDA</a>
                <a href="Iletisim.aspx">İLETİŞİM</a>
            </div>
            <div class="tasiyici ">
                <div class="logoPanel">
                    <a href="https://www.google.com/search?q=instagram/seyahatrehberii">
