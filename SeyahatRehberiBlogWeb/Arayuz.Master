                            <asp:LinkButton ID="lbtn_cikis" runat="server" OnClick="lbtn_cikis_Click">Çıkış</asp:LinkButton>
                        </asp:Panel>
                        <div class="arama">
                            <asp:TextBox ID="tb_Arama" runat="server" CssClass="AramaKutusu" placeholder="Ara..." />
